@theme "minimal-modern"

configuration{
    modi: "drun,run,window,combi";
    icon-theme: "Oranchelo";
    show-icons: true;
    terminal: "wezterm";
    drun-display-format: "{icon} {name}";
    location: 0;
    disable-history: false;
    hide-scrollbar: true;
    display-drun: "  Apps";
    display-combi: " ALL ";
    display-run: " Run ";
    display-window: " Window";
    sidebar-mode: true;
}

